generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}



model User {
  id String @default(uuid()) @unique

  firstName String @map("first_name")

  lastName String? @map("last_name")

  email String @unique

  password String

  avatar String?

  role Roles  @default(USER)

  createdAt DateTime @default(now()) @map("created_at")


  @@map("user")
}

enum Roles {
  USER
  ADMIN
}